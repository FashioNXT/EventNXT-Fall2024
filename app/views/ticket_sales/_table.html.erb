<%# Ticket Sales Table %>
<% if ticket_sales.present? %>
  <h3>Ticket Sales</h3>
  <table class="table table-striped table-hover">
    <thead class="highlight-heading">
      <tr>
        <th>Email</th>
        <th>Category</th>
        <th>Section</th>
        <th>Tickets</th>
        <th>Cost</th>
      </tr>
    </thead>
    <tbody>
      <% @ticket_sales.each do |ticket_sale| %>
        <tr>
          <td><%= ticket_sale[:email] %></td>
          <td><%= ticket_sale[:category]%></td>
          <td><%= ticket_sale[:section]%></td>
          <td><%= ticket_sale[:tickets]%></td>
          <td><%= ticket_sale[:cost] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>